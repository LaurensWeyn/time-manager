<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
       default-init-method="init">

  <!--Master context for 'user' (our) beans that we can use regardless of the 'master' context. Not that we have more than one of those right now...-->

  <!--DAO beans-->
  <bean name="userDAO" class="com.capstone.db.dao.UserDao"/>
  <bean name="CourseDAO" class="com.capstone.db.dao.CourseDao"/>
  <bean name="TimeslotDAO" class="com.capstone.db.dao.TimeslotDao"/>
  <bean name="EventDAO" class="com.capstone.db.dao.EventDao"/>
</beans>